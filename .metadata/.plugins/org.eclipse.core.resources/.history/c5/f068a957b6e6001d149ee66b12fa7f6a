package test;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
		//A = 65 a = 97
		
		int[] array = new int[25];
		String test = new String();
		
		Scanner sc = new Scanner(System.in);
		test = sc.next();
		
		// 문자열 출력 결과 확인
		System.out.println(test);
		
		System.out.println((int)(test.charAt(0)));
		
	}

}
